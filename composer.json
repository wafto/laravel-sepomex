{
    "name": "wafto/laravel-sepomex",
    "description": "Servicio Postal Mexicano (Sepomex) not official laravel library",
    "keywords": [
        "laravel",
        "sepomex",
        "mexico",
        "wafto"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wafto/laravel-sepomex/issues",
        "source": "https://github.com/wafto/laravel-sepomex"
    },
    "type": "library",
    "authors": [
        {
            "name": "Aftab Hussain Miranda",
            "email": "wafto.mx@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^8.23",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Wafto\\Sepomex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wafto\\Sepomex\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "pint": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wafto\\Sepomex\\SepomexServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
